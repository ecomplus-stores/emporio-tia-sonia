{
  "hosting": {
    "redirects": [
      {
        "source": "/coquinho-crocante",
        "destination": "/coquinho",
        "type": 301
      },
      {
        "source": "/snack-coquinho",
        "destination": "/coquinho",
        "type": 301
      },
      {
        "source": "/barra-de-cereais-tia-sonia-mix-de-castanhas-com-chocolate-display-com-24-unidades-de-25-g",
        "destination": "/barra-de-cereais-tia-sonia-mix-de-castanhas-display-com-24-unidades-de-20g",
        "type": 301
      },
      {
        "source": "/display-coquinho-crocante-tia-sonia",
        "destination": "/coquinho-crocante-25g-10-unidades",
        "type": 301
      },
      {
        "source": "/funcionais-tia-sonia-cranberry",
        "destination": "/cranberry-80g",
        "type": 301
      },
      {
        "source": "/funcionais-tia-sonia-goji-berry",
        "destination": "/granola-low-carb-com-prebiotico-nuts-e-berry-pote",
        "type": 301
      },
      {
        "source": "/gourmet-tia-sonia-mix-gourmet-pote-320g",
        "destination": "/temperado",
        "type": 301
      },
      {
        "source": "/mix-gourmet",
        "destination": "/temperado",
        "type": 301
      },
      {
        "source": "/pasta-de-amendoim",
        "destination": "/amendoin",
        "type": 301
      },
      {
        "source": "/amendoin",
        "destination": "/amendoim",
        "type": 301
      },
      {
        "source": "/sublime-tia-sonia-mix-de-castanhas-display-com-24-pacotes-de-30g",
        "destination": "/snack-mix-de-castanhas-25g--10-unidades",
        "type": 301
      },
      {
        "source": "/sublime-tia-sonia-tapioca-crocante-display-com-24-pacotes-de-30g",
        "destination": "/crocante-tapioca-25g--10-unidades",
        "type": 301
      },
      {
        "source": "/cafe-organico-colheita-das-alegrias---gourmet-torrado-graos",
        "destination": "/cafe-organico-gourmet-graos",
        "type": 301
      },
      {
        "source": "/camisa-tia-sonia-preta",
        "destination": "/todos-os-produtos",
        "type": 301
      },
      {
        "source": "/canudo-de-metal-curvo",
        "destination": "/todos-os-produtos",
        "type": 301
      },
      {
        "source": "/cranberry",
        "destination": "/cranberry-80g",
        "type": 301
      },
      {
        "source": "/display-coquinho-crocante-tia-sonia",
        "destination": "/coquinho-crocante-25g-10-unidades",
        "type": 301
      },
      {
        "source": "/funcionais",
        "destination": "/integrais",
        "type": 301
      },
      {
        "source": "/granola-tia-sonia-light-display-com-24-pacotes-de-30g",
        "destination": "/light",
        "type": 301
      },
      {
        "source": "/linha-gourmet",
        "destination": "/estilos-de-alimentacao",
        "type": 301
      },
      {
        "source": "/outros-produtos",
        "destination": "/todos-os-produtos",
        "type": 301
      },
      {
        "source": "/squeeze-tia-sonia",
        "destination": "/todos-os-produtos",
        "type": 301
      }
    ],
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**/!(*(*.)js|*(*.)css|*(*.)ico|*(*.)png|*(*.)gif|*(*.)jpg|*(*.)jpeg|*(*.)webp|*(*.)svg|*(*.)woff|*(*.)woff2|*(*.)otf|*(*.)ttf|*(*.)eot)",
        "function": "ssr"
      },
      {
        "source": "/storefront.?*.js",
        "destination": "/storefront.js"
      },
      {
        "source": "/storefront.?*.css",
        "destination": "/storefront.css"
      },
      {
        "source": "/checkout.?*.js",
        "destination": "/checkout.js"
      },
      {
        "source": "/checkout.?*.css",
        "destination": "/checkout.css"
      }
    ],
    "headers": [
      {
        "source": "/",
        "headers": [
          {
            "key": "cache-control",
            "value": "public, max-age=30, must-revalidate"
          }
        ]
      },
      {
        "source": "/img/**",
        "headers": [
          {
            "key": "cache-control",
            "value": "public, max-age=300, s-maxage=60, must-revalidate"
          }
        ]
      },
      {
        "source": "/assets/**",
        "headers": [
          {
            "key": "cache-control",
            "value": "public, max-age=86400, s-maxage=600"
          }
        ]
      },
      {
        "source": "/chunk.?*.*",
        "headers": [
          {
            "key": "cache-control",
            "value": "public, max-age=604800"
          }
        ]
      },
      {
        "source": "/app/**",
        "headers": [
          {
            "key": "cache-control",
            "value": "public, max-age=300, stale-while-revalidate=900"
          }
        ]
      },
      {
        "source": "/@(storefront|checkout).@(js|css)",
        "headers": [
          {
            "key": "cache-control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      },
      {
        "source": "**/*.?????*.@(js|css)",
        "headers": [
          {
            "key": "cache-control",
            "value": "public, max-age=86400"
          }
        ]
      }
    ],
    "cleanUrls": true
  },
  "functions": {
    "predeploy": [
      "cp -r template .functions/; rm -rf .functions/template/public; rm -rf .functions/template/scss",
      "cp -r content .functions/; cp .*.json .functions/",
      "mkdir -p .functions/template/pages/@ecomplus/storefront-template/template",
      "cp -r node_modules/@ecomplus/widget-* .functions/template/pages/@ecomplus/ 2>/dev/null || :",
      "cp -r node_modules/@ecomplus/storefront-template/template/pages .functions/template/pages/@ecomplus/storefront-template/template/ || :",
      "rm -rf .functions/template/pages/**/node_modules"
    ],
    "postdeploy": "rm -rf .functions/template; rm -rf .functions/content; rm -rf .functions/.*.json",
    "source": ".functions"
  }
}
