            // Function to make a POST request with multipart form data
            function sendDataMailbiz(event) {
              event.preventDefault();
              function postData(url, formData) {
                return fetch(url, {
                  method: 'POST',
                  body: formData,
                })
                .then(data => {
                  console.log('sending with success')
                  document.querySelector('.ml-form-successBody.row-success').style.display = 'block'
                  setTimeout(() => {
                    document.querySelector('.ml-form-successBody.row-success').style.display = 'none'
                  }, 2000)
                })
                .catch(error => console.error('Error:', error));
              }

              const form = document.getElementById('mgnr-biz');
              const formData = new FormData(form);
  
              // Make the POST request
              const apiUrl = 'https://mbiz.mailclick.me/subscribe.php';
              postData(apiUrl, formData); 
              
            }
  
